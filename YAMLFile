apiVersion: apps/v1
kind: Deployment
metadata:
  name: MyApp
spec:
  replicas: 1
  selector:
    matchLabels:
      app: MyApp
  template:
    metadata:
      labels:
        app: MyApp
    spec:
      containers:
      - name: MyApp
        image: myConregistrydemo.azurecr.io/MyApp:latest
        ports:
        - containerPort: 80
